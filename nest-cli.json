{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api",
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main-graphql",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "process-document-job": {
      "type": "application",
      "root": "apps/process-document-job",
      "entryFile": "main",
      "sourceRoot": "apps/process-document-job/src",
      "compilerOptions": {
        "tsConfigPath": "apps/process-document-job/tsconfig.app.json"
      }
    },
    "public-api": {
      "type": "application",
      "root": "apps/public-api",
      "entryFile": "main",
      "sourceRoot": "apps/public-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/public-api/tsconfig.app.json"
      }
    },
    "template-process": {
      "type": "application",
      "root": "apps/template-process",
      "entryFile": "main",
      "sourceRoot": "apps/template-process/src",
      "compilerOptions": {
        "tsConfigPath": "apps/template-process/tsconfig.app.json"
      }
    }
  }
}