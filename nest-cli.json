{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "monorepo": true,
  "root": "apps",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "index",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "public-api": {
      "type": "application",
      "root": "apps/public-api",
      "entryFile": "index",
      "sourceRoot": "apps/public-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/public-api/tsconfig.app.json"
      }
    },
    "contact-form-api": {
      "type": "application",
      "root": "apps/contact-form-api",
      "entryFile": "index",
      "sourceRoot": "apps/contact-form-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/contact-form-api/tsconfig.app.json"
      }
    },
    "output-consolidation-job": {
      "type": "application",
      "root": "apps/output-consolidation-job",
      "entryFile": "index",
      "sourceRoot": "apps/output-consolidation-job/src",
      "compilerOptions": {
        "tsConfigPath": "apps/output-consolidation-job/tsconfig.app.json"
      }
    },
    "process-document-job": {
      "type": "application",
      "root": "apps/process-document-job",
      "entryFile": "index",
      "sourceRoot": "apps/process-document-job/src",
      "compilerOptions": {
        "tsConfigPath": "apps/process-document-job/tsconfig.app.json"
      }
    },
    "credit-spending-job": {
      "type": "application",
      "root": "apps/credit-spending-job",
      "entryFile": "index",
      "sourceRoot": "apps/credit-spending-job/src",
      "compilerOptions": {
        "tsConfigPath": "apps/credit-spending-job/tsconfig.app.json"
      }
    },
    "webhook-dispatch-job": {
      "type": "application",
      "root": "apps/webhook-dispatch-job",
      "entryFile": "index",
      "sourceRoot": "apps/webhook-dispatch-job/src",
      "compilerOptions": {
        "tsConfigPath": "apps/webhook-dispatch-job/tsconfig.app.json"
      }
    }
  }
}
