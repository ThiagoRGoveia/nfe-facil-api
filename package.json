{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "sideEffects": false,
  "scripts": {
    "build:api": "ncc build apps/api/src/index.ts -o dist/apps/api",
    "build:process-document-job": "ncc build apps/process-document-job/src/index.ts -o dist/apps/process-document-job",
    "build:public-api": "ncc build apps/public-api/src/index.ts -o dist/apps/public-api",
    "build:contact-form-api": "ncc build apps/contact-form-api/src/index.ts -o dist/apps/contact-form-api",
    "build:output-consolidation-job": "ncc build apps/output-consolidation-job/src/index.ts -o dist/apps/output-consolidation-job",
    "build:credit-spending-job": "ncc build apps/credit-spending-job/src/index.ts -o dist/apps/credit-spending-job",
    "build": "rm -rf dist && pnpm build:api && pnpm build:process-document-job && pnpm build:public-api && pnpm build:contact-form-api && pnpm build:output-consolidation-job && pnpm build:credit-spending-job",
    "db-migrations:down": "mikro-orm migration:down",
    "db-migrations:generate": "mikro-orm migration:create",
    "db-migrations:initial": "mikro-orm migration:create --initial",
    "db-migrations:list": "mikro-orm migration:list",
    "db-migrations:up": "mikro-orm migration:up",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "nfse-doc:build": "ts-node --project tsconfig.json apps/api/src/infra/swagger/nfe-facil/nfe-facil-docs.ts",
    "nfse-doc:generate": "pnpm nfse-doc:build && npx @redocly/cli build-docs nfe-facil-docs.json  -o docs/nfe-facil/index.html -t mustache-templates/redoc.mustache",
    "nfse-doc:deploy": "ts-node --project tsconfig.json apps/_scripts/deploy-api-docs.ts",
    "nfse-doc:deploy:uat": "dotenv -e .env.uat -- pnpm nfse-doc:generate && dotenv -e .env.uat -- wrangler pages deploy ./docs/nfe-facil --branch uat",
    "nfse-doc:deploy:prod": "dotenv -e .env.production -- pnpm nfse-doc:generate && dotenv -e .env.production -- wrangler pages deploy ./docs/nfe-facil --branch main",
    "sst:deploy:uat": "dotenv -e .env.uat -- pnpm build && dotenv -e .env.uat -- npx sst deploy --stage uat",
    "sst:deploy:prod": "dotenv -e .env.production -- pnpm build && dotenv -e .env.production -- npx sst deploy --stage production",
    "seed:dev": "mikro-orm seeder:run --class DevSeeder",
    "api:start": "nest start api",
    "api:start:dev": "nest start api --watch",
    "api:start:debug": "nest start api --debug --watch",
    "api:start:prod": "NODE_ENV=production node dist/apps/api/index.js",
    "public-api:start": "nest start public-api",
    "public-api:start:dev": "nest start public-api --watch",
    "public-api:start:debug": "nest start public-api --debug --watch",
    "public-api:start:prod": "NODE_ENV=production node dist/apps/public-api/index.js",
    "contact-form-api:start": "nest start contact-form-api",
    "contact-form-api:start:dev": "nest start contact-form-api --watch",
    "contact-form-api:start:debug": "nest start contact-form-api --debug --watch",
    "contact-form-api:start:prod": "NODE_ENV=production node dist/apps/contact-form-api/index.js",
    "process-document-job:start": "nest start process-document-job",
    "process-document-job:start:dev": "nest start process-document-job --watch",
    "process-document-job:start:debug": "nest start process-document-job --debug --watch",
    "process-document-job:start:prod": "NODE_ENV=production node dist/apps/process-document-job/index.js",
    "output-consolidation-job:start": "nest start output-consolidation-job",
    "output-consolidation-job:start:dev": "nest start output-consolidation-job --watch",
    "output-consolidation-job:start:debug": "nest start output-consolidation-job --debug --watch",
    "output-consolidation-job:start:prod": "NODE_ENV=production node dist/apps/output-consolidation-job/index.js",
    "credit-spending-job:start": "nest start credit-spending-job",
    "credit-spending-job:start:dev": "nest start credit-spending-job --watch",
    "credit-spending-job:start:debug": "nest start credit-spending-job --debug --watch",
    "credit-spending-job:debug-explicit": "nest start --path apps/credit-spending-job/tsconfig.app.json --debug --watch",
    "credit-spending-job:start:prod": "NODE_ENV=production node dist/apps/credit-spending-job/index.js",
    "test": "jest --config jest-it.config.js",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api/test/jest-e2e.json",
    "test:process-document-job:e2e": "jest --config ./jest-e2e.config.js",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.750.0",
    "@aws-sdk/client-s3": "^3.750.0",
    "@aws-sdk/client-ses": "^3.782.0",
    "@aws-sdk/client-sqs": "^3.750.0",
    "@aws-sdk/lib-storage": "^3.777.0",
    "@aws-sdk/s3-request-presigner": "^3.777.0",
    "@codegenie/serverless-express": "^4.16.0",
    "@golevelup/nestjs-discovery": "^4.0.3",
    "@graphql-yoga/nestjs": "^3.12.1",
    "@mikro-orm/core": "^6.4.7",
    "@mikro-orm/nestjs": "^6.1.1",
    "@mikro-orm/postgresql": "^6.4.7",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.10",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.10",
    "@nestjs/graphql": "^13.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/swagger": "^11.0.5",
    "@smithy/node-http-handler": "^4.0.5",
    "@types/passport-http": "^0.3.11",
    "@types/unzipper": "^0.10.10",
    "auth0": "^4.18.0",
    "axios": "^1.7.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "graphql-scalars": "^1.24.1",
    "graphql-upload-minimal": "^1.6.1",
    "graphql-yoga": "^5.13.2",
    "https-proxy-agent": "^7.0.6",
    "install": "^0.13.0",
    "json-2-csv": "^5.5.8",
    "jwks-rsa": "^3.1.0",
    "lodash": "^4.17.21",
    "nestjs-pino": "^4.3.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.1",
    "pdf-parse": "^1.1.1",
    "pdf-to-img": "^3.0.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sst": "3.10.11",
    "stripe": "^17.7.0",
    "unzipper": "^0.12.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@faker-js/faker": "^9.5.0",
    "@golevelup/ts-jest": "^0.6.2",
    "@mikro-orm/cli": "^6.4.7",
    "@mikro-orm/migrations": "^6.4.7",
    "@mikro-orm/seeder": "^6.4.7",
    "@nestjs/cli": "^11.0.4",
    "@nestjs/schematics": "^11.0.1",
    "@nestjs/testing": "^11.0.10",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.18",
    "@types/aws-lambda": "8.10.148",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/mustache": "^4.2.5",
    "@types/node": "^22.13.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/pdf-parse": "^1.1.4",
    "@types/pluralize": "^0.0.33",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@vercel/ncc": "^0.38.3",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "mustache": "^4.2.0",
    "noop2": "^2.0.0",
    "pluralize": "^8.0.0",
    "prettier": "^3.5.2",
    "run-script-webpack-plugin": "^0.2.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.24.1",
    "webpack": "^5.98.0",
    "webpack-node-externals": "^3.0.0",
    "wrangler": "^4.16.1"
  },
  "mikro-orm": {
    "config": "./mikro-orm-config.ts"
  }
}
